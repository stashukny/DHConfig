@model IEnumerable<DHConfig.DIM_FIELD>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<h4>DIM_FIELD</h4>
<p>
    @Html.ActionLink("Create New", "Create")
</p>

@using (Html.BeginForm())
{
    <p>
        Client: @Html.DropDownList("SelectedClient", "All")
        <input type="submit" value="Filter" />
    </p>
}

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CONFIG_COMMON_NAME)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DIM_COMMON_NAME)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DIM_FIELD_NAME)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DIM_FIELD_NAME_CLEAN)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DIM_DATA_TYPE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DIM_FIELD_FEATURE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DERIVED_CONFIGURATION)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CONFIG.CONFIG_DATA_PROCESS_PROC_SCHEMA)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DIM.DIM_TABLE_SCHEMA)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CONFIG_COMMON_NAME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DIM_COMMON_NAME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DIM_FIELD_NAME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DIM_FIELD_NAME_CLEAN)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DIM_DATA_TYPE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DIM_FIELD_FEATURE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DERIVED_CONFIGURATION)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CONFIG.CONFIG_DATA_PROCESS_PROC_SCHEMA)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DIM.DIM_TABLE_SCHEMA)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { CONFIG_COMMON_NAME = item.CONFIG_COMMON_NAME, DIM_COMMON_NAME = item.DIM_COMMON_NAME, DIM_FIELD_NAME = item.DIM_FIELD_NAME }) |
            @Html.ActionLink("Details", "Details", new { CONFIG_COMMON_NAME = item.CONFIG_COMMON_NAME, DIM_COMMON_NAME = item.DIM_COMMON_NAME, DIM_FIELD_NAME = item.DIM_FIELD_NAME }) |
            @Html.ActionLink("Delete", "Delete", new { CONFIG_COMMON_NAME = item.CONFIG_COMMON_NAME, DIM_COMMON_NAME = item.DIM_COMMON_NAME, DIM_FIELD_NAME = item.DIM_FIELD_NAME })
        </td>
    </tr>
}

</table>
